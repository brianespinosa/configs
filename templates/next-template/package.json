{
  "name": "@bjeco/next-template",
  "version": "0.3.0",
  "keywords": [
    "nextjs",
    "create-next-app",
    "template"
  ],
  "homepage": "https://github.com/brianespinosa/configs/tree/main/templates/next-template",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:brianespinosa/configs.git",
    "directory": "templates/next-template"
  },
  "scripts": {
    "build": "next build",
    "lint": "eslint ./",
    "serve": "next start",
    "start": "next dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss,sass,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@bjeco/prettier-config",
  "eslintConfig": {
    "extends": "@bjeco",
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "stylelint": {
    "extends": "@bjeco/stylelint-config"
  },
  "dependencies": {
    "next": "9.5.2",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "1.26.10"
  },
  "devDependencies": {
    "@bjeco/eslint-config": "^1.2.1",
    "@bjeco/prettier-config": "^0.1.6",
    "@bjeco/stylelint-config": "^0.1.1",
    "eslint": "7.7.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "eslint-plugin-unicorn": "19.0.1",
    "husky": "1.3.1",
    "lint-staged": "10.2.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "09488b1d14c9791554bcbbc5c6453806e40ab630"
}
